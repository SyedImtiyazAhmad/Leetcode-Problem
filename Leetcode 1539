class Solution {
    public int findKthPositive(int[] arr, int k) {
      int i=0;
      int j=arr.length-1;
      if(arr[j]-j-1==0){
        return arr[j]+k;
      }
      if(arr[0]-1>=k){
        return k;
      }
      while(i<=j){
        int mid=(i+j)/2;
        if((arr[mid]-mid-1)>=k){
            j=mid-1;
        }
        else{
            i=mid+1;
        }
      }
      return arr[j] + (k - (arr[j] - j - 1));

    }
}
